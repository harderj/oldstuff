/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    // Put your code here:
    Not (in=sel[1], out=Notsel1);
    And (a=in, b=Notsel1, out=inAndNotsel1);
    And (a=in, b=sel[1], out=inAndsel1);
    DMux (in=inAndNotsel1, sel=sel[0], a=a, b=b);
    DMux (in=inAndsel1, sel=sel[0], a=c, b=d);
}